{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 11"
          ]
        },
        "useBuiltIns": "usage"
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-function-bind",
    "transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}